{
  "name": "semantic-release-backpr",
  "version": "0.0.0-dev",
  "description": "A semantic-release plugin for raising a pull-request to downstream branches",
  "type": "module",
  "module": "src/index.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsc",
    "fix": "xo --fix",
    "lint": "xo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axieum/semantic-release-backpr.git"
  },
  "keywords": [
    "backmerge",
    "downstream",
    "pr",
    "pull-request",
    "semantic-release"
  ],
  "author": "Jonathan Hiles <jonathan@hil.es>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/axieum/semantic-release-backpr/issues"
  },
  "homepage": "https://github.com/axieum/semantic-release-backpr",
  "dependencies": {
    "@semantic-release/error": "^4.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/lodash": "^4.17.7",
    "@types/semantic-release__error": "^3.0.3",
    "xo": "^0.59.0"
  },
  "peerDependencies": {
    "semantic-release": ">=23",
    "typescript": "^5.0.0"
  },
  "xo": {
    "space": true,
    "rules": {
      "n/file-extension-in-import": "off",
      "unicorn/no-array-reduce": "off"
    }
  }
}
